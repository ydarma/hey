HeyGrammar {
  Hey =
    Range | Square | Fun | value

  Range =
    "range" "(" value value value ")"

  Square =
    "square" "(" value value ")"
    
  Fun =
    argList ":" Hey
    
  argList = "(" listOf<(identifier | argList), " "> ")"

  value = number | identifier | color
    
  color =
    "green" | "blue" | "yellow" | "red" | "purple" | "grey" | "black" | "white"

  number = digit+

  identifier = (alnum | "-")+
}