HeyGrammar {
  Hey =
    Range | Square | Fun

  Range =
    "range" "(" arg arg arg ")"

  Square =
    "square" "(" arg arg ")"
    
  Fun =
    argList ":" Hey
    
  argList = "(" listOf<(arg | argList), " "> ")"
  arg = number | identifier | color
    
  color =
    "green" | "blue" | "yellow" | "red" | "purple" | "grey" | "black" | "white"

  number = digit+

  identifier = (alnum | "-")+
}